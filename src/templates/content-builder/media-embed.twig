{% if embedUrl.errors|length == 0 %}
    {% for url in embedUrl.urlInfo %}
        <div class="embed mb-2">
            <div class="aspect-{{ embedRatio }}"></div>
            <iframe class="" title="{{ url.title }}" frameborder="0" allowfullscreen src="{{ url.iframeEmbed.src }}" loading="lazy"></iframe>
        </div>
    {% endfor %}
{% else %}
    {{ embedUrl.errors|join('<br>') }}
{% endif %}
